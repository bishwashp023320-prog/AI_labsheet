   "source": [
    "from sklearn import svm\n",
    "\n",
    "X = [[1,2], [2,3], [3,3], [6,5]]\n",
    "y = [0, 0, 1, 1]\n",
    "\n",
    "model = svm.SVC(kernel='linear')\n",
    "model.fit(X, y)\n",
    "\n",
    "print(\"Prediction:\", model.predict([[2,2]]))\n"
   ]
